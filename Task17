//Q, Write a C program to print the HCF
of two numbers

    
    
#include<stdio.h>
int main() {
int a, b, hcf, i; 
scanf("%d\n%d",&a, &b);
if(a>=b){
    for(i=b;i>=1;i--){
      if(a%i==0 && b%i==0){
        hcf = i;break;}}
        }
        else{
        for(i=a;i>=1;i--){
          hcf = i;break;}}}
        printf("%d",i);
        
        return 0:
        }
        
        
        
