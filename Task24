//Q, Write a C program to find the frequency 
//of odd and even numbers in the 1D
//array. 

#include<stdio.h>
int main() {
int i, n,myarray[10],even=0,odd=0;
scanf("%d", &n);
for(i=0;i<n;i++)
{
  scanf("%d", &myarray[i]);
}
for(i=0;i<n;i++)
{
  if(myarray[i]%2==0)
  {
    even++;
   }
  else 
  {
   odd++;
  }
 }
  printf("%d\n%d ",even, odd); 
  return 0;
}
